{
    "openapi": "3.0.0",
    "info": {
        "title": "API Documentation",
        "version": "1.0"
    },
    "paths": {
        "/api/auth/login": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Get a JWT via given credentials",
                "description": "Get a JWT via given credentials",
                "operationId": "333609af984061e6db1ea838f74312b8",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "email",
                                    "password"
                                ],
                                "properties": {
                                    "email": {
                                        "description": "User email",
                                        "type": "string",
                                        "format": "email",
                                        "example": ""
                                    },
                                    "password": {
                                        "description": "User password",
                                        "type": "string",
                                        "format": "password",
                                        "example": ""
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Credentials are valid",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "description": "JWT token",
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV..."
                                        },
                                        "token_type": {
                                            "description": "Token type",
                                            "type": "string",
                                            "example": "bearer"
                                        },
                                        "expires_in": {
                                            "description": "Token expiration time in seconds",
                                            "type": "integer",
                                            "example": "3600"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/api/auth/logout": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Log the user out (Invalidate the token)",
                "description": "Log the user out (Invalidate the token)",
                "operationId": "899c25a4680f3bec181377daf36e62d5",
                "responses": {
                    "200": {
                        "description": "Successfully logged out",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Successfully logged out"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Unauthorized | Error while decoding the token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/auth/refresh": {
            "post": {
                "tags": [
                    "Authentication"
                ],
                "summary": "Refresh a token",
                "description": "Refresh a token",
                "operationId": "699449f0d835a5229deff4f9b66a54d2",
                "responses": {
                    "200": {
                        "description": "Token refreshed",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "access_token": {
                                            "description": "JWT token",
                                            "type": "string",
                                            "example": "eyJ0eXAiOiJKV..."
                                        },
                                        "token_type": {
                                            "description": "Token type",
                                            "type": "string",
                                            "example": "bearer"
                                        },
                                        "expires_in": {
                                            "description": "Token expiration time in seconds",
                                            "type": "integer",
                                            "example": "3600"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Unauthorized | Error while decoding the token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v1/subaccounts": {
            "get": {
                "tags": [
                    "Subaccounts"
                ],
                "summary": "Get all subaccounts for the authenticated account user",
                "description": "Get all subaccounts for the authenticated account user",
                "operationId": "d66a5f5111182c8d3c21e1beb0708fb7",
                "responses": {
                    "200": {
                        "description": "Subaccounts retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "subaccounts": {
                                            "properties": {
                                                "page": {
                                                    "description": "Current page",
                                                    "type": "integer",
                                                    "example": "1"
                                                },
                                                "total_pages": {
                                                    "description": "Total pages",
                                                    "type": "integer",
                                                    "example": "1"
                                                },
                                                "total_records": {
                                                    "description": "Total records",
                                                    "type": "integer",
                                                    "example": "1"
                                                }
                                            },
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "UUID": {
                                                        "description": "Subaccount UUID",
                                                        "type": "string",
                                                        "example": "123456"
                                                    },
                                                    "ExternalId": {
                                                        "description": "Subaccount ExternalId",
                                                        "type": "string",
                                                        "example": "123456"
                                                    },
                                                    "Description": {
                                                        "description": "Subaccount Description",
                                                        "type": "string",
                                                        "example": "My subaccount"
                                                    },
                                                    "Balance": {
                                                        "description": "Subaccount Balance",
                                                        "type": "string",
                                                        "example": "0.00"
                                                    },
                                                    "STPAccount": {
                                                        "description": "STPAccount associated with the subaccount",
                                                        "type": "string",
                                                        "example": "123456"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Unauthorized | Error while decoding the token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            },
            "post": {
                "tags": [
                    "Subaccounts"
                ],
                "summary": "Create a new subaccount",
                "description": "Create a new subaccount",
                "operationId": "6950fa9808f9369daceab2e7638b48b2",
                "requestBody": {
                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "ExternalId",
                                    "Description"
                                ],
                                "properties": {
                                    "ExternalId": {
                                        "type": "string",
                                        "example": "123456"
                                    },
                                    "Description": {
                                        "type": "string",
                                        "example": "My subaccount"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "200": {
                        "description": "Subaccount created successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "UUID": {
                                            "description": "Subaccount UUID",
                                            "type": "string",
                                            "example": "123456"
                                        },
                                        "ExternalId": {
                                            "description": "Subaccount ExternalId",
                                            "type": "string",
                                            "example": "123456"
                                        },
                                        "Description": {
                                            "description": "Subaccount Description",
                                            "type": "string",
                                            "example": "My subaccount"
                                        },
                                        "Balance": {
                                            "description": "Subaccount Balance",
                                            "type": "string",
                                            "example": "0.00"
                                        },
                                        "STPAccount": {
                                            "description": "STPAccount associated with the subaccount",
                                            "type": "string",
                                            "example": "123456"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Unauthorized | Error while decoding the token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Subaccount with this ExternalId already exists | Subaccount with this Description already exists | Error creating subaccount",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
                                            "type": "string",
                                            "example": "Subaccount with this ExternalId already exists | Subaccount with this Description already exists | Error creating subaccount"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/api/v1/subaccounts/{uuid}": {
            "get": {
                "tags": [
                    "Subaccounts"
                ],
                "summary": "Get a specific subaccount",
                "description": "Get a specific subaccount",
                "operationId": "ad73511451a322cf87e9d5a2dd5d43ef",
                "parameters": [
                    {
                        "name": "uuid",
                        "in": "path",
                        "description": "Subaccount UUID",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "example": "123456"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Subaccount retrieved successfully",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "UUID": {
                                            "description": "Subaccount UUID",
                                            "type": "string",
                                            "example": "123456"
                                        },
                                        "ExternalId": {
                                            "description": "Subaccount ExternalId",
                                            "type": "string",
                                            "example": "123456"
                                        },
                                        "Description": {
                                            "description": "Subaccount Description",
                                            "type": "string",
                                            "example": "My subaccount"
                                        },
                                        "Balance": {
                                            "description": "Subaccount Balance",
                                            "type": "string",
                                            "example": "0.00"
                                        },
                                        "STPAccount": {
                                            "description": "STPAccount associated with the subaccount",
                                            "type": "string",
                                            "example": "123456"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    },
                    "401": {
                        "description": "Unauthorized",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "description": "Message",
